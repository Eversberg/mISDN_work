# Makefile for the modular ISDN driver
#
EXTRA_CFLAGS += -ggdb
#

ifdef MINCLUDES
EXTRA_CFLAGS += -I$(MINCLUDES) -g
endif

EXTRA_CFLAGS += -DMISDNVERSION=\"$(MISDNVERSION)\"

ifdef CONFIG_MISDN_HFCPCI
obj-$(CONFIG_MISDN) += hfcpci.o
endif

ifdef CONFIG_MISDN_HFCUSB
obj-$(CONFIG_MISDN) += hfcsusb.o
endif

ifdef CONFIG_MISDN_HFCMULTI
obj-$(CONFIG_MISDN) += hfcmulti.o
endif

ifdef CONFIG_MISDN_XHFC
obj-$(CONFIG_MISDN) += xhfc.o
endif

ifdef CONFIG_MISDN_HWSKEL
obj-$(CONFIG_MISDN) += mISDN_hwskel.o
endif

ifdef CONFIG_MISDN_AVMFRITZ
obj-$(CONFIG_MISDN) += avmfritz.o
endif

ifdef CONFIG_MISDN_L1LOOP
obj-$(CONFIG_MISDN) += mISDN_l1loop.o
endif

ifdef CONFIG_MISDN_ISAC
obj-$(CONFIG_MISDN) += mISDNisac.o
endif

# multi objects

xhfc-objs := xhfc_su.o xhfc_pci2pi.o
mISDN_hwskel-objs := hwskel.o
mISDN_l1loop-objs := l1loop.o
